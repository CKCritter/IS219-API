const express = require("express");
const router = express.Router();
const hwController = require("../controllers/hwController.js")

//Create
router.post('/', hwController.post);

//Read
router.get('/', hwController.getAll);

//Read
router.get('/:id', hwController.getById);

//Update
router.put('/:id', hwController.put);

//Delete
router.delete('/:id', hwController.delete);

module.exports = router;